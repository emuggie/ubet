<% form_for @user do |f| %>
    <p>name</p>
    <%=f.text_field :name%>
    <p>email</p>
    <%=f.text_field :email%>
    <p>password</p>
    <%= f.password_field :password%>
    <p>password confirmation</p>
    <%= f.password_field :password_confirmation%>
    <p>user_photo</p>
    <%= f.file_field :user_photo %>
    <%=f.submit :register%>
<% end %>

